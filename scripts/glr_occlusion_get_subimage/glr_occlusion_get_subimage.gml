/// @description glr_occlusion_get_subimage(id);
/// @param id
function glr_occlusion_get_subimage(argument0) {

	/*
	    <=============== DESCRIPTION ===============>
	        Returns the subimage of the occlusion 
	    <===========================================>
	*/

	var l = argument0;
	return l[| GLR_OCCLUSION.subimage];



}
